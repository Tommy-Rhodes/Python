message = input(">")
words = message.split(" ")
emojis = {
    ":)": "ğŸ˜Š",
    "<3": "â¤",
    ":(": "â˜¹",
    ":/": "ğŸ˜",
    ":P": "ğŸ˜›",
    ":'(": "ğŸ˜¥",
    ":D": "ğŸ˜€",
    ":O": "ğŸ˜®",
    ";P": "ğŸ˜œ",
    ">:(": "'ğŸ˜¤"
}
output = ""
for word in words:
    output += emojis.get(word, word) + " "
print(output)
